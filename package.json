  "repository": {
    "type": "git",
    "url": "https://github.com/gaurangtorvekar/x402-devkit"
  },
  "homepage": "https://github.com/gaurangtorvekar/x402-devkit#readme",
  "bugs": {
    "url": "https://github.com/gaurangtorvekar/x402-devkit/issues"
  }
